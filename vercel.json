{
  "version": 2,
  "buildCommand": "pnpm -w run vercel-build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(?!api/).*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }

}
