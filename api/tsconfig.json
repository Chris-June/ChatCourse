{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2020",
    "lib": ["ES2020"],
    "types": ["node", "express"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": false
  },
  "include": [
    "**/*.ts",
    "../apps/api/**/*.ts"
  ]
}
